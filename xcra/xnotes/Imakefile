#include "../config.mk"

           SRCS = Note.C XawNote.C XawNoteList.C XawNoteMgr.C
        HEADERS = Note.H XawNote.H XawNoteList.H XawNoteMgr.H
           OBJS = Note.o XawNote.o XawNoteList.o XawNoteMgr.o

     CCOPTIONS  = IncludeSearchPaths SpecificOptions X11Version
    CDEBUGFLAGS = DebugOptions WarnOptions OptimizeOptions
    DEPENDFLAGS = $(CCOPTIONS) DependOptions

NormalLibraryTarget(notes,$(OBJS))
